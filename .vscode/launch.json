{
    "version": "0.1.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Server",
            "program": "${workspaceFolder}/server/index.js",
            "console": "integratedTerminal",
        },
        {
            "name": "Launch Client",
            "type": "chrome",
            "request": "launch",
